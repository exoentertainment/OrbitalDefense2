						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float3 desaturateInitialColor95 = _TintA.rgb;
				float desaturateDot95 = dot( desaturateInitialColor95, float3( 0.299, 0.587, 0.114 ));
				float3 desaturateVar95 = lerp( desaturateInitialColor95, desaturateDot95.xxx, _TintA.a );
				float3 desaturateInitialColor96 = _TintB.rgb;
				float desaturateDot96 = dot( desaturateInitialColor96, float3( 0.299, 0.587, 0.114 ));
				float3 desaturateVar96 = lerp( desaturateInitialColor96, desaturateDot96.xxx, _TintB.a );
				float2 uv_Base = IN.ase_texcoord3.xy * _Base_ST.xy + _Base_ST.zw;
				float4 tex2DNode1 = tex2D( _Base, uv_Base );
				float3 lerpResult109 = lerp( desaturateVar95 , desaturateVar96 , tex2DNode1.r);
				
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
				float3 worldSpaceViewDir63 = ( _WorldSpaceCameraPos.xyz - mul(GetObjectToWorldMatrix(), float4( 0,0,0,0 ) ).xyz );
				float3 normalizeResult65 = normalize( worldSpaceViewDir63 );
				float dotResult66 = dot( ase_worldNormal , normalizeResult65 );
				float eyeDepth = IN.ase_texcoord3.z;
				float cameraDepthFade48 = (( eyeDepth -_ProjectionParams.y - _DistFadeOffset ) / _DistFadeLength);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = saturate( ( saturate( lerpResult109 ) * tex2DNode1.r ) );
				float Alpha = saturate( ( saturate( ( saturate( pow( abs( dotResult66 ) , _FadeViewAngleExp ) ) * _FadeViewAngleMult ) ) * saturate( cameraDepthFade48 ) * IN.ase_color.a ) );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
